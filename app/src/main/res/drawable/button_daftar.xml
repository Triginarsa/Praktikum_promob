<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <solid
                android:color="#0F112C"/>
            <stroke
                android:width="2dp"
                android:color="#0F112C" />
        </shape>
    </item>
    <item>
        <shape>
            <solid
                android:color="#00000000"/>
            <stroke
                android:width="2dp"
                android:color="#0F112C" />
        </shape>
    </item>
</selector>